entity Supplier {
	ID Integer,
    Name String required,
    DateCreated LocalDate,
    Active Boolean,
    RegNumber String
}
entity SupplierType{
    ID Integer,
    Name Integer,
    Redistributor Boolean
}

relationship OneToOne {
	Supplier{supplierType} to SupplierType
}

// Set pagination options
paginate Supplier, SupplierType with pagination